{
  "dependencies": {
    "react": "^18.1.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.1.0",
    "reactjs-popup": "^2.0.5"
  },
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development webpack serve --open --config webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=production webpack serve --open --config webpack.prod.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.dev.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "eslint": "npx eslint \"src/**\" --fix",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.5",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-modal": "^3.13.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cypress": "^10.3.0",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.30.0",
    "formik": "^2.2.9",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "react-multi-select-component": "^4.2.9",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^13.0.0",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0",
    "yup": "^0.32.11"
  },
  "eslintIgnore": [
    "*.scss"
  ],
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ]
  }
}
